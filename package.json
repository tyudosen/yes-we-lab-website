{
  "name": "ywl-website",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "ci": "payload migrate && pnpm build",
    "db:create:migration": "payload migrate:create"
  },
  "dependencies": {
    "@payloadcms/admin-bar": "3.37.0",
    "@payloadcms/db-postgres": "3.37.0",
    "@payloadcms/live-preview-react": "3.37.0",
    "@payloadcms/next": "3.37.0",
    "@payloadcms/payload-cloud": "3.37.0",
    "@payloadcms/plugin-form-builder": "3.37.0",
    "@payloadcms/plugin-nested-docs": "3.37.0",
    "@payloadcms/plugin-redirects": "3.37.0",
    "@payloadcms/plugin-search": "3.37.0",
    "@payloadcms/plugin-seo": "3.37.0",
    "@payloadcms/richtext-lexical": "3.37.0",
    "@payloadcms/storage-uploadthing": "^3.37.0",
    "@payloadcms/ui": "3.37.0",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-progress": "^1.1.6",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-slot": "^1.2.2",
    "@tailwindcss/postcss": "^4.1.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "effect": "^3.14.20",
    "geist": "^1.4.2",
    "graphql": "^16.11.0",
    "lucide-react": "^0.507.0",
    "mysharp": "npm:sharp@^0.34.1",
    "next": "15.3.1",
    "next-sitemap": "^4.2.3",
    "payload": "3.37.0",
    "prism-react-renderer": "^2.4.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "7.56.2",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/escape-html": "^1.0.4",
    "@types/node": "22.15.12",
    "@types/react": "19.1.3",
    "@types/react-dom": "19.1.3",
    "autoprefixer": "^10.4.21",
    "copyfiles": "^2.4.1",
    "eslint": "^9.26.0",
    "eslint-config-next": "15.3.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.5",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "mysharp"
    ]
  }
}
